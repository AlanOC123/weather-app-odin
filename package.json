{
  "name": "project-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/**/*.js' --fix",
    "format": "prettier --write 'src/**/*.{js,css,html}'",
    "format-check": "prettier --check 'src/**/*.{js,css,html}'",
    "prepare": "husky install",
    "update-deps": "ncu -u",
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "webpack build --config webpack/webpack.prod.js",
    "setup": "npm install",
    "on-start": "npm install && npm run build && npm start"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "Alan O'Connor",
  "license": "ISC",
  "description": "Setup for future projects as a base. Uses Google standard for linting and formatting.",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/eslintrc": "^3.2.0",
    "babel": "^6.23.0",
    "babel-loader": "^9.2.1",
    "commitizen": "^4.3.1",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-check-updates": "^17.1.13",
    "prettier": "^3.4.2",
    "style-loader": "^4.0.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "css-minimizer-webpack-plugin": "^7.0.0",
    "date-fns": "^4.1.0",
    "normalize.css": "^8.0.1",
    "pexels": "^1.4.0",
    "postcss-loader": "^8.1.1",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.97.1"
  }
}
